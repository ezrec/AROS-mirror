+IPTR WebPreferences__OM_DISPOSE(struct IClass *cl, Object *obj, Msg msg)
+{
+    struct WebPreferences_DATA *data = (struct WebPreferences_DATA *) INST_DATA(cl, obj);
+    if(data->cookieJarFileName)
