maintainer-clean-local:
	echo "all:" > Makefile
	echo -e "\t$(ACLOCAL)" >> Makefile
	echo -e "\tautoheader" >> Makefile
	echo -e "\tautoconf" >> Makefile
	echo -e "\tautomake --foreign" >> Makefile
	echo -e "\tconfigure --enable-maintainer-mode --prefix=/home/carlo" >> Makefile

