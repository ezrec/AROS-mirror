USER_CXXFLAGS := $(NOWARN_CXX_FLAGS) -DHAVE_CONFIG_H

%build_linklib mmake=contrib-arospdf-goo libname=Goo \
  files=$(C_FILES) cxxfiles=$(CXX_FILES) libdir=$(TARGETDIR)

%common
