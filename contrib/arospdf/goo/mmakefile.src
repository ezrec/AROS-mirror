CXX_FILES = \
	GHash \
	GList \
	GString \
	gmem \
	gmempp \
	gfile \
	FixedPoint

C_FILES = \
	parseargs

TARGETDIR := $(AROS_CONTRIB)/Utilities/Text/AROSPDF/libs
USER_INCLUDES := -I$(SRCDIR)/$(CURDIR)/..
NOWARN_C_FLAGS := $(NOWARN_WRITE_STRINGS)
