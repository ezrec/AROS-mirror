include $(TOP)/config/make.cfg

#MM contrib-gfx-libs-cairo-boiler : contrib-gfx-libs-cairo

FILES := cairo-boilerplate cairo-boilerplate-aros

USER_INCLUDES := -I$(SRCDIR)/$(CURDIR)/../aros -I$(SRCDIR)/$(CURDIR)/../src \
	-I$(GENINCDIR)/pixman-1
USER_CFLAGS := -DHAVE_CONFIG_H -DCAIRO_NO_MUTEX -D__BSD_VISIBLE

%build_linklib mmake=contrib-gfx-libs-cairo-boiler libname=boiler \
  files="$(FILES)" libdir=$(AROS_TESTS)/cairo

%common
